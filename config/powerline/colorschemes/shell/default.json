{
	"name": "Default color scheme for shell prompts",
	"groups": {
		"hostname":         { "fg": "brightyellow", "bg": "mediumorange", "attrs": [] },
		"environment":      { "fg": "white", "bg": "darkestgreen", "attrs": [] },
		"mode":             { "fg": "darkestgreen", "bg": "brightgreen", "attrs": ["bold"] },
		"date": 						{ "fg": "black", "bg": "brightyellow", "attrs": ["bold"] },
		"attached_clients": { "fg": "white", "bg": "darkestgreen", "attrs": [] },
    "gitstatus":                 { "fg": "black",           "bg": "gray7", "attrs": ["bold"] },
    "gitstatus_branch":          { "fg": "black",           "bg": "gray7", "attrs": ["bold"] },
    "gitstatus_branch_clean":    { "fg": "brightgreen",    "bg": "gray7", "attrs": ["bold"] },
    "gitstatus_branch_dirty":    { "fg": "black",           "bg": "gray7", "attrs": ["bold"] },
    "gitstatus_branch_detached": { "fg": "mediumpurple",    "bg": "gray7", "attrs": ["bold"] },
    "gitstatus_tag":             { "fg": "darkcyan",        "bg": "gray7", "attrs": ["bold"] },
    "gitstatus_behind":          { "fg": "black",           "bg": "gray7", "attrs": ["bold"] },
    "gitstatus_ahead":           { "fg": "black",           "bg": "gray7", "attrs": ["bold"] },
    "gitstatus_staged":          { "fg": "brightgreen",    "bg": "gray7", "attrs": ["bold"] },
    "gitstatus_unmerged":        { "fg": "darkred",       "bg": "gray7", "attrs": ["bold"] },
    "gitstatus_changed":         { "fg": "darkred",       "bg": "gray7", "attrs": ["bold"] },
    "gitstatus_untracked":       { "fg": "yellow",    "bg": "gray7", "attrs": ["bold"] },
    "gitstatus_stashed":         { "fg": "darkblue",        "bg": "gray7", "attrs": ["bold"] },
    "gitstatus:divider":         { "fg": "black",           "bg": "gray7", "attrs": [] }
	},
	"mode_translations": {
		"vicmd": {
			"groups": {
				"mode": {"fg": "darkestcyan", "bg": "white", "attrs": ["bold"]}
			}
		}
	}
}
