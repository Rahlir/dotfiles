COMPLETIONDIR="/usr/share/bash-completion"
DIRCOLORSDIR=$HOME/.dircolors/dircolors.base16
export POWERLINEDIR=$HOME/anaconda3/lib/python3.6/site-packages/powerline

# OS specific aliases
alias ls='ls --color'
alias copyc="tr -d '\n' | xclip -selection clipboard -i"
alias clip='xclip -selection clipboard -i'
alias bashrcl='vim ~/.bashrc_linux'
alias grep='grep --color'

# Added by Anaconda3 5.0.0 installer
export PATH="$HOME/anaconda3/bin:$PATH"

